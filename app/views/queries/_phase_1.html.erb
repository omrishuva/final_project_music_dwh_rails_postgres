<div class="container">
  <div class="row-fluid pagination-centered">
    <div class="span12">    
      <h3><%= "select fact :" unless tables.size == 1 %><h3>
      <% tables.each do |fact|%>
        <span3>
           <span1>
             <label class="label" >
                <%=link_to(fact.name,queries_path(params.merge({fact:fact.name}))) %>
             </label>
           </span1>
        </span3>
      <%end%>
    </div>
  </div>  
</div>  
