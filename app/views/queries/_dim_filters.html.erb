<% dims.keys.each do |dim| %>
  <%=render partial: "dim_attributes", locals:{dims:dims, dim: dim} %>
     <% if dims[dim][:snow_dims].present?%>
      <%snow = dims[dim][:snow_dims].keys.first%>
      <%=render partial: "dim_attributes", locals:{dims: dims[dim][:snow_dims], dim:snow} %>
  <%end%>
<%end%>
